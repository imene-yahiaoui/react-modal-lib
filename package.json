{
  "name": "modal-react-vite-ts",
  "description": "A modal component for React or Typescript applications.",
  "private": false,
  "version": "0.1.1",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/modal.umd.cjs",
  "module": "./dist/modal.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/modal.js",
      "require": "./dist/modal.umd.cjs"
    },
    "./dist/style.css": "./dist/index.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && copyfiles -u 1 src/*.css dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "modal-react-vite-ts": "^0.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-css-injected-by-js": "^3.5.1"
  },
  "keywords": [
    "vite",
    "react",
    "typescript",
    "modal-react-vite-ts",
    "modal",
    "react applications",
    "modal whith react"
  ],
  "author": {
    "name": "Imene Ikhlef Yahiaoui"
  },
  "licence": "ISC",
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imene-yahiaoui/react-modal-lib"
  }
}
